{
	"name": "cli-debugger",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"cli": "dist/cli.js",
	"server": "dist/server.js",
	"targets": {
		"main": false,
		"cli": {
			"source": "src/cli.js",
			"context": "node",
			"outputFormat": "esmodule",
			"isLibrary": true,
			"optimize": false,
			"includeNodeModules": false
		},
		"server": {
			"source": "src/server.js",
			"context": "node",
			"outputFormat": "esmodule",
			"isLibrary": true,
			"optimize": false,
			"includeNodeModules": false
		}
	},
	"alias": {
		"./events": "./src/events/index.js",
		"./data": "./src/data.js",
		"./utils": "./src/utils/index.js",
		"./hooks": "./src/hooks/index.js",
		"./components": "./src/components/index.js",
		"./events.test": "./src/test/events/index.js",
		"./utils.test": "./src/test/utils/index.js",
		"./hooks.test": "./src/test/hooks/index.js",
		"./components.test": "./src/test/components/index.js"
	},
	"scripts": {
		"clearAll": "rm -r ./.parcel-cache ./dist",
		"clear": "rm -r ./dist",
		"start": "dist/cli.js --enable-source-maps",
		"experiment": "dist/cli.js --enable-source-maps --experiment",
		"build": "parcel build",
		"watch": "parcel watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^1.0.0",
		"@vercel/webpack-asset-relocator-loader": "^1.7.3",
		"acorn": "^8.11.2",
		"ansi-escapes": "^6.2.0",
		"asciify": "^1.3.5",
		"dotenv": "^16.3.1",
		"ink": "^4.1.0",
		"ink-select-input": "^5.0.0",
		"ink-text-input": "^5.0.1",
		"meow": "^11.0.0",
		"node-loader": "^2.0.0",
		"react": "^18.2.0",
		"shelljs": "^0.8.5",
		"webpack-node-externals": "^3.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.23.6",
		"@babel/plugin-syntax-import-attributes": "^7.23.3",
		"@babel/preset-env": "^7.23.6",
		"@babel/preset-react": "^7.23.3",
		"@parcel/resolver-glob": "^2.10.3",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"babel-loader": "^9.1.3",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"import-jsx": "^5.0.0",
		"ink-testing-library": "^3.0.0",
		"parcel": "^2.10.3",
		"prettier": "^2.8.7",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"xo": "^0.53.1"
	},
	"ava": {
		"environmentVariables": {
			"NODE_NO_WARNINGS": "1"
		},
		"nodeArguments": [
			"--loader=import-jsx"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
